{
  "name": "@infisical/sdk",
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Infisical <team@infisical.com> (https://infisical.com)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/infisical/sdk"
  },
  "files": [
    "binding.js",
    "binding.d.ts",
    "./lib/",
    "./lib/infisical_client/"
  ],
  "napi": {
    "name": "infisical",
    "defaults": true,
    "triples": {
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "arm-linux-androideabi",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build": "yarn --cwd ../../crates/infisical-napi build --target $npm_config_target && npm run move && npm run format-binding",
    "build-arm64": "npm run build-arm64 --prefix ../../crates/infisical-napi && npm run move && npm run format-binding",
    "build-gnu-arm64": "npm run build-gnu-arm64 --prefix ../../crates/infisical-napi && npm run move && npm run format-binding",
    "build-win-ia32": "npm run build-win-ia32 --prefix ../../crates/infisical-napi && npm run move-win-ia32 && npm run format-binding",
    "move-win-ia32": "npm run move-win-ia32:binaries && npm run move:win-ia32:bindings",
    "move": "npm run move:bindings && npm run move:binaries",
    "move:win-ia32:bindings": "move ../../crates/infisical-napi/binding.* ./",
    "move:bindings": "mv -v ../../crates/infisical-napi/binding.* ./",
    "move-win-ia32:binaries": "move ../../crates/infisical-napi/infisical.win32-ia32-msvc.node ./",
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish --skip-gh-release",
    "move:binaries": "mv -v ../../crates/infisical-napi/*.node ./",
    "copy:artifacts": "cp ./artifacts/schemas.ts ./src/infisical_client/schemas.ts && cp ./artifacts/binding.* ./",
    "tsc": "tsc",
    "format-binding": "node ./format-binding.js",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.13.2",
    "ts-node": "10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tinyify": "4.0.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}