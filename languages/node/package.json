{
  "name": "@infisical/sdk",
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Infisical <team@infisical.com> (https://infisical.com)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/infisical/sdk"
  },
  "files": [
    "binding.js",
    "binding.d.ts",
    "./lib/",
    "./lib/infisical_client/"
  ],
  "napi": {
    "name": "infisical",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "i686-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build": "npm run build --prefix ../../crates/infisical-napi && npm run move",
    "build-arm64": "npm run build-arm64 --prefix ../../crates/infisical-napi && npm run move",
    "build-win-ia32": "npm run build-win-ia32 --prefix ../../crates/infisical-napi && npm run move-win-ia32",
    "move-win-ia32": "npm run move-win-ia32:binaries && npm run move:win-ia32:bindings",
    "move": "npm run move:bindings && npm run move:binaries",
    "move:win-ia32:bindings": "move ../../crates/infisical-napi/binding.* ./",
    "move:bindings": "mv -v ../../crates/infisical-napi/binding.* ./",
    "move-win-ia32:binaries": "move ../../crates/infisical-napi/infisical.win32-ia32-msvc.node ./",
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish --skip-gh-release",
    "move:binaries": "mv -v ../../crates/infisical-napi/*.node ./",
    "copy:artifacts": "cp ./artifacts/schemas.ts ./src/infisical_client/schemas.ts && cp ./artifacts/binding.* ./",
    "tsc": "tsc",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.13.2",
    "ts-node": "10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "tinyify": "4.0.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "optionalDependencies": {
    "@infisical/sdk-win32-x64-msvc": "0.0.0",
    "@infisical/sdk-darwin-x64": "0.0.0",
    "@infisical/sdk-linux-x64-gnu": "0.0.0",
    "@infisical/sdk-darwin-arm64": "0.0.0",
    "@infisical/sdk-win32-ia32-msvc": "0.0.0"
  }
}